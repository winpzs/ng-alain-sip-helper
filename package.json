{
  "name": "ngalainsiphelper",
  "displayName": "ng-alain-sip Helper",
  "description": "Run ng-alain-sip Helper from vscode commandline.",
  "icon": "images/angular.png",
  "version": "1.0.2",
  "publisher": "winpzs",
  "bugs": {
    "url": "https://github.com/winpzs/ng-alain-sip-helper/issues",
    "email": "winpzs@gmail.com"
  },
  "homepage": "https://github.com/winpzs/ng-alain-sip-helper/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/winpzs/ng-alain-sip-helper.git"
  },
  "categories": [
    "Languages",
    "Snippets",
    "Other"
  ],
  "engines": {
    "vscode": "^1.5.0"
  },
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "Angular",
    "Angular 5",
    "ng-alain-sip",
    "TypeScript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      }
    ],
    "commands": [
      {
        "command": "ngalainsiphelper.tosnippettext",
        "title": "To Snippet Text"
      },
      {
        "command": "ngalainsiphelper.quickpicks",
        "title": "ng-alain-sip"
      },
      {
        "command": "ngalainsiphelper.region",
        "title": "Region Block"
      }
    ],
    "keybindings": [
      {
        "command": "ngalainsiphelper.quickpicks",
        "key": "ctrl+k ctrl+h",
        "mac": "cmd+k cmd+h",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "",
          "command": "ngalainsiphelper.quickpicks",
          "group": "ng-alain-sip"
        }
      ],
      "editor/context": [
        {
          "when": "",
          "command": "ngalainsiphelper.quickpicks",
          "group": "ng-alain-sip"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:package": "vsce package",
    "vscode:publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^2.0.0",
    "@types/node": "^8.0.0",
    "mocha": "^4.0.0",
    "run-in-terminal": "^0.0.3",
    "rxjs": "^5.0.3",
    "typescript": "^2.0.0",
    "vscode": "^1.1.0"
  },
  "dependencies": {
    "json-stable-stringify": "^1.0.1",
    "yargs-parser": "^8.1.0"
  }
}